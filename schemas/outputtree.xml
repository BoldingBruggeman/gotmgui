<?xml version="1.0"?>
<element name="root">
	<element name="turbulence" label="turbulent quantities">
		<element name="tke"/>
		<element name="eps"/>
		<element name="eps_obs">
			<condition type="eq" variable="/obs/eprofile" value="2" source="scenario"/>
		</element>
		<element name="l"/>
		<element name="num"/>
		<element name="nuh"/>
		<element name="nus"/>
		
		<element name="buoyancy" label="buoyancy">
			<element name="G"/>
			<element name="NN"/>
			<element name="NN_obs">
				<condition type="or" source="scenario">
					<condition type="ne" variable="/obs/sprofile" value="0"/>
					<condition type="ne" variable="/obs/tprofile" value="0"/>
				</condition>
			</element>
			<element name="kb"/>
			<element name="Pb"/>
			<element name="epsb"/>
			<element name="sigma_t"/>
			<element name="sigma_t_obs"/>
		</element>

		<element name="shear" label="shear">
			<element name="P"/>
			<element name="SS"/>
			<element name="SS_obs">
				<condition type="ne" variable="/obs/ext_pressure" value="0" source="scenario"/>
			</element>
			<element name="uu"/>
			<element name="vv"/>
			<element name="ww"/>
		</element>
	</element>
	<element name="tracers" label="tracers">
		<element name="temp"/>
		<element name="temp_obs">
			<condition type="ne" variable="/obs/tprofile" value="0" source="scenario"/>
		</element>
		<element name="salt"/>
		<element name="salt_obs">
			<condition type="ne" variable="/obs/sprofile" value="0" source="scenario"/>
		</element>
		<element name="o2_obs">
			<condition type="ne" variable="/obs/o2_profile" value="0" source="scenario"/>
		</element>
	</element>
	<element name="velocities" label="velocities">
		<element name="u"/>
		<element name="u_obs">
			<condition type="ne" variable="/obs/ext_pressure" value="0" source="scenario"/>
		</element>
		<element name="v"/>
		<element name="v_obs">
			<condition type="ne" variable="/obs/ext_pressure" value="0" source="scenario"/>
		</element>
	</element>
	<element name="boundaries" label="boundaries">
		<element name="sst">
			<condition type="or" source="scenario">
				<condition type="eq" variable="/airsea/flux_source" value="0"/>
				<condition type="ne" variable="/airsea/sst_method"  value="0"/>
			</condition>
		</element>
		<element name="sss">
			<condition type="and" source="scenario">
				<condition type="eq" variable="/airsea/flux_source" value="1"/>
				<condition type="ne" variable="/airsea/sss_method"  value="0"/>
			</condition>
		</element>
		<element name="p_e">
			<condition type="ne" variable="/airsea/p_e_method" value="0" source="scenario"/>
		</element>
		<element name="x-taus"/>
		<element name="y-taus"/>
		<element name="u_taus"/>
		<element name="u_taub"/>
		<element name="swr"/>
		<element name="int_swr"/>
		<element name="heat"/>
		<element name="int_heat"/>
		<element name="total"/>
		<element name="int_total"/>
		<element name="zeta">
			<condition type="or" source="scenario">
				<condition type="ne" variable="/obs/zetaspec"        value="0"/>
				<condition type="ne" variable="/obs/zetaspec/zeta_0" value="0"/>
			</condition>
		</element>
	</element>
	<element name="column" label="column structure">
		<element name="h"/>
	</element>
	<element name="nonlocal" label="non-local fluxes" hidden="True">
		<element name="gamh"/>
		<element name="gams"/>
		<element name="gamu"/>
		<element name="gamv"/>
	</element>
	<element name="biogeochemistry">
		<element name="npzd" grouponly="True">
			<condition type="and" source="scenario">
				<condition type="eq" variable="/bio/bio_calc"  value="True"/>
				<condition type="eq" variable="/bio/bio_model" value="1"/>
			</condition>
			<element name="nut"/>
			<element name="phy"/>
			<element name="zoo"/>
			<element name="det"/>
		</element>
		<element name="iow" grouponly="True">
			<condition type="and" source="scenario">
				<condition type="eq" variable="/bio/bio_calc"  value="True"/>
				<condition type="eq" variable="/bio/bio_model" value="2"/>
			</condition>
			<element name="dia"/>
			<element name="fla"/>
			<element name="cya"/>
			<element name="zoo"/>
			<element name="det"/>
			<element name="amm"/>
			<element name="nit"/>
			<element name="pho"/>
			<element name="oxy"/>
			<element name="flf">
				<condition type="eq" variable="/bio/bio_model/bio_iow/fluff" value="True" source="scenario"/>
			</element>
		</element>
		<element name="sed" grouponly="True">
			<condition type="and" source="scenario">
				<condition type="eq" variable="/bio/bio_calc"  value="True"/>
				<condition type="eq" variable="/bio/bio_model" value="3"/>
			</condition>
			<element name="conc"/>
		</element>
		<element name="fasham" grouponly="True">
			<condition type="and" source="scenario">
				<condition type="eq" variable="/bio/bio_calc"  value="True"/>
				<condition type="eq" variable="/bio/bio_model" value="4"/>
			</condition>
			<element name="phy"/>
			<element name="zoo"/>
			<element name="bac"/>
			<element name="det"/>
			<element name="nit"/>
			<element name="amm"/>
			<element name="ldn"/>
		</element>
	</element>
	<element name="seagrass">
		<condition type="eq" variable="/seagrass/canopy/seagrass_calc" value="True" source="scenario"/>
		<element name="x-excur"/>
		<element name="y-excur"/>
	</element>
	<element name="other" label="other"/>
</element>